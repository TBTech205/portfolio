generator client {
  provider = "prisma-client-js"

}

datasource db {
  provider = "sqlite"
  url      = "file:../analytics.db"
}

model Dev {
  id        Int      @id @default(autoincrement())
  token     String   @unique
  createdAt DateTime @default(now())
}

model Visitor {
  id        Int      @id @default(autoincrement())
  ip        String
  visitedAt DateTime @default(now())
}

model LinkClick {
  id        Int      @id @default(autoincrement())
  url       String
  clickedAt DateTime @default(now())
}